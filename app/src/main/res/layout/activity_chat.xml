<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#AB58B658"
    tools:context=".Chat">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#AB58B658"
        tools:context=".Chat"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="-28dp">

        <TextView
            android:id="@+id/textView13"
            android:layout_width="348dp"
            android:layout_height="89dp"
            android:layout_marginTop="90dp"
            android:background="#29972D"
            android:fontFamily="sans-serif-thin"
            android:text="@string/pm1"
            android:textColor="#290F0F"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/bdgt"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/bdgt"
            android:layout_width="352dp"
            android:layout_height="58dp"
            android:layout_marginStart="4dp"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="8dp"
            android:background="#29972D"
            android:fontFamily="sans-serif-thin"
            android:text="@string/pm2"
            android:textColor="#290F0F"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/nameT"
            app:layout_constraintStart_toStartOf="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/textView13"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="319dp"
            android:layout_height="62dp"
            android:layout_marginStart="4dp"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="16dp"
            android:background="#29972D"
            android:fontFamily="sans-serif-thin"
            android:text="@string/pidnum2"
            android:textColor="#290F0F"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/weightTt"
            app:layout_constraintStart_toStartOf="@+id/textView29"
            app:layout_constraintTop_toBottomOf="@+id/BdayTt"
            app:layout_constraintVertical_bias="0.038" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="357dp"
            android:layout_height="81dp"
            android:layout_marginTop="16dp"
            android:background="#29972D"
            android:fontFamily="sans-serif-thin"
            android:text="@string/pm5"
            android:textColor="#290F0F"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/textView30"
            app:layout_constraintTop_toBottomOf="@+id/weightTt"
            app:layout_constraintVertical_bias="0.304" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="359dp"
            android:layout_height="86dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="16dp"
            android:background="#29972D"
            android:fontFamily="sans-serif-thin"
            android:text="@string/pm4"
            android:textColor="#290F0F"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/BdayTt"
            app:layout_constraintStart_toStartOf="@+id/bdgt"
            app:layout_constraintTop_toBottomOf="@+id/nameT"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/nameT"
            android:layout_width="268dp"
            android:layout_height="52dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="24dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="@string/prcode"
            app:layout_constraintEnd_toEndOf="@+id/BdayTt"
            app:layout_constraintTop_toBottomOf="@+id/bdgt" />

        <EditText
            android:id="@+id/weightTt"
            android:layout_width="251dp"
            android:layout_height="51dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="48dp"
            android:layout_marginRight="48dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="@string/pidnum1"
            app:layout_constraintEnd_toEndOf="@+id/conf"
            app:layout_constraintTop_toBottomOf="@+id/textView30" />

        <EditText
            android:id="@+id/BdayTt"
            android:layout_width="254dp"
            android:layout_height="46dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="@string/pn1"
            app:layout_constraintEnd_toEndOf="@+id/weightTt"
            app:layout_constraintTop_toBottomOf="@+id/textView29" />

        <Button
            android:id="@+id/conf"
            android:layout_width="215dp"
            android:layout_height="78dp"
            android:layout_marginTop="60dp"
            android:text="@string/btnCertify"
            android:textColor="#D3BBBB"
            app:backgroundTint="#041B05"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.928"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView31"
            app:layout_constraintVertical_bias="0.582" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>