<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_establishment">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="551dp"
        android:layout_height="523dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.742"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button12"
        app:srcCompat="@drawable/photo" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="966dp"
        android:layout_height="966dp"
        android:layout_marginBottom="250dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.534"
        app:srcCompat="@drawable/circle" />

    <Button
        android:id="@+id/button10"
        android:layout_width="345dp"
        android:layout_height="77dp"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="-25dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:text="Care Home Registration"
        android:textColor="#91008A"
        android:textSize="20sp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/button11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.42000002" />

    <Button
        android:id="@+id/button11"
        android:layout_width="345dp"
        android:layout_height="77dp"
        android:background="#FFFFFF"
        android:text="Dhana Reservation"
        android:textColor="#91008A"
        android:gravity="center"
        android:textSize="20sp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/button12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button10" />

    <Button
        android:id="@+id/button12"
        android:layout_width="345dp"
        android:layout_height="77dp"
        android:layout_marginBottom="275dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:text="Registered care Homes"
        android:textColor="#91008A"
        android:textSize="20sp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button11" />




</androidx.constraintlayout.widget.ConstraintLayout>